class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int maxi = nums[0], currSubArray = nums[0];
        for (int i=1;i<nums.size();i++)
        {
            currSubArray = max(currSubArray+nums[i], nums[i]);
            maxi = max(maxi, currSubArray);
        }
        return maxi;
    }
};
